<div class="container">
  <div class="header-container">
    <div class="title-bar clearfix">
      <h1 class="title">Band List</h1>
    </div>
    <nav>
      <div class="nav-wrapper">
        <a href="/set_lists" class="nav-button">See All Set Lists</a>
        <a href="/songs" class="nav-button">See All Songs</a>
      </div>
    </nav>
  </div>
  <div class="content-wrapper">
    <div class="upcoming-wrapper text-center">
      <h3>Upcoming Shows</h3>
      <% @set_lists.each do |set_list| %>
        <a href="/set_lists/<%=set_list.id%>" class="set-list-item"><%=set_list.venue%> <%=set_list.performance_date%></a>
      <% end %>
    </div>
  </div>
</div>
